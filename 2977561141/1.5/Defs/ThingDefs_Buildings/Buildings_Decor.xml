<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="FurnitureWithQualityBase">
		<defName>CCol_BucketPlantPot</defName>
		<label>plant bucket</label>
		<description>A repurposed bucket, built from scratch for some reason, that holds soil to grow decorative flowers. The handle is fixed in place, don’t question it.</description>
		<thingClass>Building_PlantGrower</thingClass>
		<graphicData>
			<texPath>Things/Building/Decor/BucketPlantPot/BucketPlantPot</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shadowData>
				<volume>(0.55, 0.35, 0.3)</volume>
				<offset>(0, 0, -0.28)</offset>
			</shadowData>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.20</fillPercent>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>250</WorkToBuild>
			<Mass>2</Mass>
			<Flammability>1.0</Flammability>
		</statBases>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<comps>
			<li Class="CompProperties_Forbiddable" />
			<li Class="CompProperties_Styleable" />
		</comps>
		<fertility>1.0</fertility>
		<designationHotKey>Misc9</designationHotKey>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<building>
			<defaultPlantToGrow>Plant_Daylily</defaultPlantToGrow>
			<sowTag>Decorative</sowTag>
		</building>
		<rotatable>false</rotatable>
		<uiIconOffset>(0, -0.05)</uiIconOffset>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
	</ThingDef>

