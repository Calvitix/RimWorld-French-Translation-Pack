            <name>DankPyon_IronIngot</name>
          </match>
        </li>

        <!-- *-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*
          BiosolidsComposter
        -*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-* -->
        <li Class="PatchOperationConditional">
          <xpath>/Defs/ThingDef[defName="BiosolidsComposter"]/costList/Steel</xpath>
          <match Class="PatchOperationSetName">
            <xpath>/Defs/ThingDef[defName="BiosolidsComposter"]/costList/Steel</xpath>
            <name>DankPyon_IronIngot</name>
